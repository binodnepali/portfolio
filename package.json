{
  "version": "1.0.0",
  "description": "Portfolio website",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build": "vue-tsc --noEmit && vite build",
    "dev": "vite",
    "release": "standard-version",
    "serve": "vite preview",
    "prepare": "husky install",
    "lint": "eslint ./src/**/*.vue --fix",
    "lint:style": "stylelint ./src/**/*.vue --custom-syntax postcss-html  --fix"
  },
  "dependencies": {
    "open-props": "^1.1.5",
    "vue": "^3.0.5"
  },
  "devDependencies": {
    "@babel/types": "^7.16.8",
    "@typescript-eslint/parser": "^5.10.2",
    "@vitejs/plugin-vue": "^1.2.3",
    "@vue/compiler-sfc": "^3.0.5",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.2",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "standard-version": "^9.3.2",
    "stylelint": "^14.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "typescript": "^4.1.3",
    "vite": "^2.3.5",
    "vue-eslint-parser": "^8.2.0",
    "vue-tsc": "^0.0.24"
  },
  "lint-staged": {
    "**/**/*.vue": [
      "eslint --fix",
      "stylelint --custom-syntax postcss-html  --fix",
      "prettier --write"
    ]
  },
