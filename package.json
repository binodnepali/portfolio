{
  "name": "portfolio",
  "description": "My portfolio website build with Nuxt 3 and Openprops. And it also uses css, scss and sass",
  "keywords": [
    "nuxt",
    "nuxt3",
    "openprops",
    "css",
    "scss",
    "sass",
    "portfolio",
    "personal",
    "portfolio-website",
    "personal-website"
  ],
  "private": true,
  "version": "1.0.0",
  "engineStrict": true,
  "engines": {
    "node": ">=16.11.0"
  },
  "scripts": {
    "dev": "nuxi dev",
    "build": "nuxi build",
    "start": "node .output/server/index.mjs",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lint:fix": "eslint --ext .ts,.js,.vue . --fix",
    "lint:style": "stylelint **/*.{css,scss,sass,html,vue}",
    "lint:style:fix": "stylelint **/*.{css,scss,sass,html,vue} --fix",
    "prepare": "husky install",
    "commit": "git-cz",
    "release": "standard-version",
    "release:first": "yarn release --first-release",
    "deploy:firebase": "NITRO_PRESET=firebase yarn build && firebase deploy"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^17.2.0",
    "@commitlint/config-conventional": "^17.2.0",
    "@commitlint/cz-commitlint": "^17.2.0",
    "@material-design-icons/svg": "latest",
    "@nuxtjs/eslint-config-typescript": "^11.0.0",
    "commitizen": "^4.2.5",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "firebase-admin": "^10.0.2",
    "firebase-functions": "^3.19.0",
    "firebase-functions-test": "^0.3.3",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "nuxt": "3.0.0-rc.12",
    "open-props": "^1.3.9",
    "postcss": "^8.4.19",
    "postcss-html": "^1.5.0",
    "postcss-jit-props": "^1.0.7",
    "prettier": "^2.7.1",
    "sass": "^1.49.9",
    "standard-version": "^9.5.0",
    "stylelint": "^14.15.0",
    "stylelint-config-idiomatic-order": "^9.0.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-standard": "^29.0.0",
    "typescript": "^4.8.4",
    "vite-svg-loader": "^3.6.0"
  }
}
